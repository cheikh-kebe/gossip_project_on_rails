<div class= "form ">
<%= form_tag url_for(action: 'create'), method: "post" do %>

<p><%= label_tag 'Email' %></p>
  <%= text_field_tag 'email' %>
  <br /> <br />

  <p><%= label_tag 'Mot de passe' %></p>
  <%= text_field_tag 'password_digest' %>
  <br /> <br />

  <%= submit_tag "Connexion" %>
<% end %>
</div>